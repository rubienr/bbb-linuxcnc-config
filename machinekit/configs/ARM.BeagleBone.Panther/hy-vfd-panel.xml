          <tablerow/>
            <tablesticky sticky="w" />
            <label>
              <text>"Max Current [A]"</text>
            </label>
            <tablesticky sticky="e" />
            <number> 
              <halpin>"hy-rated-motor-current"</halpin>
              <format>"4.1f"</format>
            </number>

          <tablerow/>
            <tablesticky sticky="w" />
            <label>
              <text>"Current [A]"</text>
            </label>
            <tablesticky sticky="e" />
            <number> 
              <halpin>"hy-outa"</halpin>
              <format>"4.1f"</format>
            </number>
            
          <tablerow/>
            <tablespan columns="2" />
            <tablesticky sticky="nsew" />
            <label>
              <text>" "</text>
              <font>("Helvetica",2)</font>
            </label>

          <tablerow/>
            <tablespan columns="2" />
            <tablesticky sticky="nsew" />
            <label>
              <text>"Frequency Specs"</text>
            </label>

          <tablerow/>
            <tablesticky sticky="w" />
            <label>
              <text>"Max. Freq. [Hz]"</text>
            </label>
            <tablesticky sticky="e" />
            <number> 
              <halpin>"hy-max-freq"</halpin>
              <format>"4.1f"</format>
            </number>
              
          <tablerow/>
            <tablesticky sticky="w" />
            <label>
              <text>"Min. Freq. [Hz]"</text>
            </label>
            <tablesticky sticky="e" />
            <number> 
              <halpin>"hy-freq-lower-limit"</halpin>
              <format>"4.1f"</format>
            </number>

          <tablerow/>
            <tablespan columns="2" />
            <tablesticky sticky="nsew" />
            <label>
              <text>" "</text>
              <font>("Helvetica",2)</font>
            </label>

          <tablerow/>
            <tablespan columns="2" />
            <tablesticky sticky="nsew" />
            <label>
              <text>"Output"</text>
            </label>

          <tablerow/>
            <tablesticky sticky="w" />
            <label>
              <text>"Commanded Freq. [Hz]"</text>
            </label>
            <tablesticky sticky="e" />
            <number> 
              <halpin>"hy-setf"</halpin>
              <format>"4.1f"</format>
            </number>

          <tablerow/>
            <tablesticky sticky="w" />
            <label>
              <text>"Out Freq. [Hz]"</text>
            </label>
            <tablesticky sticky="e" />
            <number> 
              <halpin>"hy-outf"</halpin>
              <format>"4.1f"</format>
            </number>

          <tablerow/>
            <tablespan columns="2" />
            <tablesticky sticky="nsew" />
            <label>
              <text>" "</text>
              <font>("Helvetica",2)</font>
            </label>

          <tablerow/>
            <tablespan columns="2" />
            <tablesticky sticky="nsew" />
            <label>
              <text>"VFD"</text>
            </label>
            
          <tablerow/>
            <tablesticky sticky="w" />
            <label>
              <text>"DC [V]"</text>
            </label>
            <tablesticky sticky="e" />
            <number> 
              <halpin>"hy-dcv"</halpin>
              <format>"4.1f"</format>
            </number>
            
          <tablerow/>
            <tablesticky sticky="w" />
            <label>
              <text>"Temp. [Â°C]"</text>
            </label>
            <tablesticky sticky="e" />
            <number> 
              <halpin>"hy-tmp"</halpin>
              <format>"4.1f"</format>
            </number>
            
          <tablerow/>
            <tablespan columns="2" />
            <tablesticky sticky="nsew" />
            <label>
              <text>" "</text>
              <font>("Helvetica",2)</font>
            </label>
        </table>
      </labelframe>

      <labelframe text="Commanded">
        <font>("Helvetica",12)</font>
        <table>
          <tablerow/>
            <tablesticky sticky="w" />
            <label>
              <text>"Speed [r/m]"</text>
            </label>
            <tablesticky sticky="e" />
            <number> 
              <halpin>"hy-speed-command"</halpin>
              <format>"4.1f"</format>
            </number>
                  
          <tablerow/>
            <tablesticky sticky="w" />
            <label>
              <text>"Frequency 100*[Hz]"</text>
            </label>
            <tablesticky sticky="e" />
            <number> 
              <halpin>"hy-frequency-command"</halpin>
              <format>"4.1f"</format>
            </number>

          <tablerow/>
            <tablespan columns="2" />
            <tablesticky sticky="nsew" />
            <label>
              <text>" "</text>
              <font>("Helvetica",2)</font>
            </label>
        </table>
      </labelframe>
    </vbox>
      
    <vbox>
      <labelframe text="Control">
        <font>("Helvetica",12)</font>
        <table>
          <tablerow/>
            <tablespan columns="2" />
            <tablesticky sticky="nsew" />
            <label>
              <text>" "</text>
              <font>("Helvetica",2)</font>
            </label>

          <tablerow/>
            <tablesticky sticky="w" />
            <label>
              <text>"Loop Time [s]"</text>
            </label>
            <tablesticky sticky="e" />
            <number> 
              <halpin>"hy-loop-time"</halpin>
              <format>"1.3f"</format>
            </number>
            
          <tablerow/>
            <tablesticky sticky="w" />
            <label>
              <text>"Counter"</text>
            </label>
            <tablesticky sticky="e" />
            <number> 
              <halpin>"hy-cont"</halpin>
              <format>"4.1f"</format>
            </number>
            
          <tablerow/>
            <tablesticky sticky="w" />
            <label>
              <text>"Control Request Reg."</text>
            </label>
            <tablesticky sticky="e" />
            <s32> 
              <halpin>"hy-cntr"</halpin>
              <format>"3d"</format>
            </s32>

          <tablerow/>
            <tablesticky sticky="w" />
            <label>
                <text>"Control Response Reg."</text>
            </label>
            <tablesticky sticky="e" />
            <s32> 
                <halpin>"hy-cnst"</halpin>
                <format>"3d"</format>
            </s32>

          <tablerow/>
            <tablesticky sticky="w" />
            <label>
                <text>"Run rev./fwd."</text>
            </label>
            <tablesticky sticky="e" />
            <led>
              <halpin>"hy-cnst-command-rf"</halpin>
              <size>"10"</size>
              <on_color>"green"</on_color>
              <off_color>"red"</off_color>
            </led>
                
          <tablerow/>
            <tablesticky sticky="w" />
            <label>
              <text>"Run"</text>
            </label>
            <tablesticky sticky="e" />
            <led>
              <halpin>"hy-cnst-run"</halpin>
              <size>"10"</size>
              <on_color>"green"</on_color>
              <off_color>"red"</off_color>
            </led>
                
          <tablerow/>
            <tablesticky sticky="w" />
            <label>
              <text>"Running"</text>
            </label>
            <tablesticky sticky="e" />
            <led>
              <halpin>"hy-cnst-running"</halpin>
              <size>"10"</size>
              <on_color>"green"</on_color>
              <off_color>"red"</off_color>
            </led>
              
          <tablerow/>
            <tablesticky sticky="w" />
            <label>
              <text>"Jog rev./fwd."</text>
            </label>
            <tablesticky sticky="e" />
            <led>
              <halpin>"hy-cnst-running-rf"</halpin>
              <size>"10"</size>
              <on_color>"green"</on_color>
              <off_color>"red"</off_color>
            </led>
              
          <tablerow/>
            <tablesticky sticky="w" />
            <label>
              <text>"Jog"</text>
            </label>
            <tablesticky sticky="e" />
            <led>
              <halpin>"hy-cnst-jog"</halpin>
              <size>"10"</size>
              <on_color>"green"</on_color>
              <off_color>"red"</off_color>
            </led>
              
          <tablerow/>
            <tablesticky sticky="w" />
            <label>
              <text>"Jogging"</text>
            </label>
            <tablesticky sticky="e" />
            <led>
              <halpin>"hy-cnst-jogging"</halpin>
              <size>"10"</size>
              <on_color>"green"</on_color>
              <off_color>"red"</off_color>
            </led>

          <tablerow/>
            <tablesticky sticky="w" />
            <label>
              <text>"Braking"</text>
            </label>
            <tablesticky sticky="e" />
            <led>
              <halpin>"hy-cnst-bracking"</halpin>
              <size>"10"</size>
              <on_color>"green"</on_color>
              <off_color>"red"</off_color>
            </led>

          <tablerow/>
            <tablesticky sticky="w" />
            <label>
              <text>"Track Start"</text>
            </label>
            <tablesticky sticky="e" />
            <led>
              <halpin>"hy-cnst-track-start"</halpin>
              <size>"10"</size>
              <on_color>"green"</on_color>
              <off_color>"red"</off_color>
            </led>

          <tablerow/>
            <tablespan columns="2" />
            <tablesticky sticky="nsew" />
            <label>
              <text>" "</text>
              <font>("Helvetica",2)</font>
            </label>
        </table>
      </labelframe>
    </vbox>
  </tabs>
</pyvcp>
